plotm <- function(data,mtitle)#
{#
	xmaxlim <- max(data$bn)#
	op <- par(mfrow = c(2,1), oma = c(0,0,3,0))#
	plot(1,1, type = "n", xlim = c(0,xmaxlim), ylim = c(0,10), xlab = "Boot Number", ylab = " Mean Fitness")#
	lines(data$bn, data$MP1Fa, col = 4, lty = 2)#
	lines(data$bn, data$MP1Fb, col = 2, lty = 2)#
	lines(data$bn, data$MP1Aa, col = 4, lty = 1)#
	lines(data$bn, data$MP1Ab, col = 2, lty = 1)#
	title("Plant 1")#
#
	plot(1,1, type = "n", xlim = c(0,xmaxlim), ylim = c(0,10), xlab = "Boot Number", ylab = "Mean Fitness")#
	lines(data$bn, data$MP2Fa, col = 4, lty = 2)#
	lines(data$bn, data$MP2Fb, col = 2, lty = 2)#
	lines(data$bn, data$MP2Aa, col = 4, lty = 1)#
	lines(data$bn, data$MP2Ab, col = 2, lty = 1)#
	title("Plant 2")#
	#
	par(op)#
	mtext(mtitle, side=3, line=1.5, cex=1.5)#
	par(op)#
}#
#
plot.run <- function(data,mtitle)#
{#
	ut <- unique(data$time)#
	outm <- c()#
#
	for(i in ut)#
	{	#
		temp1 <- data[data$t == i & data$ptype == 1,]#
		temp2 <- data[data$t == i & data$ptype == 2,]#
		tout <- c(i,length(temp1$ptype), length(temp2$ptype), mean(temp1$fit_a), mean(temp1$fit_b),mean(temp1$attract_a), mean(temp1$attract_b),mean(temp2$fit_a), mean(temp2$fit_b),mean(temp2$attract_a), mean(temp2$attract_b))#
		outm <- rbind(outm,tout)#
	}#
#
	outm <- data.frame(outm)#
#
	names(outm) <- c("t", "NT1", "NT2", "T1Fa", "T1Fb", "T1Aa", "T1Ab","T2Fa", "T2Fb", "T2Aa", "T2Ab")#
	#
	xlimmax <- max(outm$t)#
#
	op <- par(mfrow = c(2,1), oma = c(0,0,3,0))#
	plot(1,1, type = "n", xlim = c(0,xlimmax), ylim = c(0,10), xlab = "Time", ylab = "Fitness")#
	lines(outm$t, outm$T1Fa, col = 4, lty = 2)#
	lines(outm$t, outm$T1Fb, col = 2, lty = 2)#
	lines(outm$t, outm$T1Aa, col = 4, lty = 1)#
	lines(outm$t, outm$T1Ab, col = 2, lty = 1)#
	title("Plant 1")#
#
	plot(1,1, type = "n", xlim = c(0,xlimmax), ylim = c(0,10), xlab = "Time", ylab = 	"Fitness")#
	lines(outm$t, outm$T2Fa, col = 4, lty = 2)#
	lines(outm$t, outm$T2Fb, col = 2, lty = 2)#
	lines(outm$t, outm$T2Aa, col = 4, lty = 1)#
	lines(outm$t, outm$T2Ab, col = 2, lty = 1)#
	title("Plant 2")#
	#
	par(op)#
	mtext(mtitle, side=3, line=1.5, cex=1.5)#
	par(op)#
}#
